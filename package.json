{
    "name": "filesharing",
    "version": "0.1.0",
    "engines": {
        "node": "18 || 20 || 22 || 24"
    },
    "type": "module",
    "private": true,
    "dependencies": {
        "@craco/craco": "^7.1.0",
        "@fluentui/react": "^8.123.1",
        "@fluentui/react-components": "^9.55.1",
        "@fluentui/react-icons": "^2.0.305",
        "@microsoft/teams-js": "^2.31.1",
        "@microsoft/teamsfx": "^3.0.0",
        "@microsoft/teamsfx-react": "^4.0.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^30.0.0",
        "@types/styled-components": "^5.1.34",
        "autoprefixer": "^10.4.21",
        "axios": "^0.21.1",
        "lucide-react": "^0.525.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.8.0",
        "styled-components": "^6.1.19",
        "tailwindcss": "^4.1.11",
        "web-vitals": "^5.0.3"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@types/react-router-dom": "^5.3.3",
        "@vitejs/plugin-basic-ssl": "^1.1.0",
        "@vitejs/plugin-react": "^4.3.1",
        "concurrently": "^8.2.2",
        "env-cmd": "^10.1.0",
        "typescript": "^4.1.2",
        "vite": "^5.4.2",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^1.6.0",
        "jsdom": "^24.1.0"
    },
    "scripts": {
        "dev:teamsfx": "concurrently \"npm run dev-tab:teamsfx\" \"npm run dev-api:teamsfx\"",
        "dev-tab:teamsfx": "env-cmd --silent -f .localConfigs npm run start",
        "dev-api:teamsfx": "cd api && npm run dev:teamsfx",
        "start": "vite",
        "build": "tsc && vite build",
        "test": "vitest run --environment=jsdom",
        "serve": "vite preview"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "homepage": "."
}
